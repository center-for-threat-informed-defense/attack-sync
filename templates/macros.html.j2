{%- macro change_details_table(values) -%}
<table class="change-details-table">
  <tr>
    <th>Field</th>
    <th>Old value</th>
    <th>New value</th>
  </tr>
  {%- for value in values -%}
  <tr>
    <td>{{value.stix}}</td>
    <td>{{value.old}}</td>
    <td>{{value.new}}</td>
  </tr>
  {%- endfor -%}
</table>
{%- endmacro %}
